<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".solutionActivity" android:visibility="visible">

    <TextView
            android:text="Введенное уравнение имеет следующие корни:"
            android:layout_width="286dp"
            android:layout_height="79dp" android:id="@+id/textView"
            android:textSize="18sp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.025"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.018" android:textAlignment="textStart"/>
    <TextView
            android:layout_width="354dp"
            android:layout_height="70dp" android:id="@+id/textView3"
            android:textSize="18sp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintTop_toBottomOf="@+id/textView"/>
    <TextView
            android:layout_width="351dp"
            android:layout_height="71dp" android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintVertical_bias="0.0" android:textSize="18sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>